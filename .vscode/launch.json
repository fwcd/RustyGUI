	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug rustygui",
			"cargo": {
				"args": [
					"build",
					"--bin=rustygui"
				]
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug tests in rustygui",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=rustygui"
				]
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}
